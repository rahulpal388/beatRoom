generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String  @id @default(uuid())
  email      String  @unique
  password   String  @default("null")
  UserId     String  @unique
  isVerified Boolean @default(false)
  profile    String?
}

model Otp {
  id        String   @id @default(uuid())
  otp       String
  createdAt DateTime
  expireAt  DateTime
}
